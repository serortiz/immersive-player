{
    "env": {
        "browser": true,
        "es2022": true,
        "commonjs": false,
        "jest/globals": true
    },
    "plugins": ["jest"],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "ignorePatterns": ["**/dist/*","**/docs/*"],
    "rules": {
        "template-curly-spacing":1,
        "no-useless-call":2,
        "no-throw-literal":2,
        "no-return-await":2,
        "no-unreachable-loop":1,
        "no-unsafe-optional-chaining":1,
        "no-multiple-empty-lines":1,
        "no-labels":2,
        "no-implied-eval":2,
        "guard-for-in":2,
        "no-import-assign":2,
        "no-loop-func":2,
        "prefer-spread":1,
        "no-duplicate-imports":1,
        "no-lonely-if":1,
        "prefer-object-spread":1,
        "no-array-constructor":1,
        "no-undef-init":1,
        "no-return-assign":1,
        "no-extra-bind":2,
        "no-lone-blocks":1,
        "no-else-return":1,
        "no-caller":2,
        "no-unused-vars":1,
        "no-empty-function":2,
        "default-param-last":1,
        "array-callback-return":2,
        "no-template-curly-in-string":2,
        "no-extra-parens":1,
        "no-useless-concat":1,
        "no-buffer-constructor":2,
        "no-var":2,
        "nonblock-statement-body-position":1,
        "prefer-const":2,
        "indent": [ 1, 4 ],
        "max-statements": 0,
        "eqeqeq": [2,"smart"],
        "linebreak-style": [ 2, "unix" ],
        "semi": [ 1, "always" ],
        "semi-spacing": 2,
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs"],
        "camelcase": 0,
        "eol-last": 2,
        "new-cap": 1,
        "no-extend-native": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-trailing-spaces": 1,
        "no-multi-spaces": 2,
        "no-use-before-define": [2, "nofunc"],
        "quotes":["warn","double", { "avoidEscape": true }],
        "space-unary-ops": 2,
        "keyword-spacing": 1,
        "space-before-function-paren": 0,
        "space-in-parens": 1,
        "strict": [0],
        "no-underscore-dangle": 0,
        "comma-dangle": ["warn", "never"],
        "no-console": 1
    }
}
